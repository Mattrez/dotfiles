alt + Return
	$TERMINAL

alt + w
	$TERMINAL -e tmux a -t weechat

alt + shift + m
	$TERMINAL -e "mutt"

alt + n
	$TERMINAL -e "mocp"

alt + shift + f
	$TERMINAL -e "vfzf"

alt + p
	dmenu_run

alt + b
	brave

alt + s
	spotify

alt + c
	clipmenu

alt + shift + p
	poweroff

alt + Escape
	pkill -USR1 -x sxhkd

XF86AudioRaiseVolume
	amixer sset Master 2%+

XF86AudioLowerVolume
	amixer sset Master 2%-

XF86MonBrightnessUp
	xbacklight -inc 5

XF86MonBrightnessDown
	xbacklight -dec 5
